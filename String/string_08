#include <stdio.h>
#include <string.h>
#define MAX 100

    // função que escreve um numero que é digitado como entrada.

    void escreve_numero (char num[]) {
        
       int n, aux;
       
       n=strlen(num)-1;
       
       for(int i=0; i<=n; i++) {
       
       switch (num[i]) {
        case '1':
        printf("um,");
        break;
        case '2':
        printf("dois,");
        break;
        case '3':
        printf("tres,");
        break;
        case '4':
        printf("quatro,");
        break;
        case '5':
        printf("cinco,");
        break;
        case '6':
        printf("seis,");
        break;
        case '7':
        printf("sete,");
        break;
        case '8':
        printf("oito,");
        break;
        case '9':
        printf("nove,");
        break;
        case '0':
        printf("zero,");
        break;
       }
    } 
}   
    
int main () {
    
    char num [MAX];
    scanf("%[^\n]%*c", num);
    escreve_numero (num);
    
    return 0;
}
