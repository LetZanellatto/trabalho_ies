#include <stdio.h>

    void lervet (int vx[]) {//lÃª o vetor
    int i;
    for (i = 0; i < 10; i++)
    scanf ("%d", &vx[i]);
}

    int getmaior (int vx[]) {//encontra e retorna o maior numero do vetor
    int i,maior = vx[0];
    for (i = 0;i < 10;i++){
    if (vx[i] > maior) 
    maior = vx[i];
    }
    return maior;
}

    int getmenor (int vx[]) { encontra e retorna o menor numero do vetor
    int i; int m;
    for (m = vx[0], i = 1; i <= 10; i++){
    if (vx[i] < m) m = vx[i];
    }
    return m;
}

int main() {
    
    int va[100];
    lervet (va);
    printf ("%d\n", getmaior(va));
    printf ("%d\n", getmenor(va));

    return 0;
}
