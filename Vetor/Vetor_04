#include <stdio.h>

void lervet (double vx[]) {
    int i;
   
    for (i = 0; i <10; i++)
    scanf ("%lf", &vx[i]);
}
void printvet (double vx[]) {
    int i;
    for (i = 10-1; i >= 0; i--)
    printf ("%.2lf\n", vx[i]);
}
int main() {
    double v[10];
    double n;
    lervet (v);
    printf ("\n");
    printvet (v);

    return 0;
}
